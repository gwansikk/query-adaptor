{
  "name": "@query-fetch/core",
  "version": "0.5.1",
  "description": "Lightweight and Easy-to-Use Fetch API Extension Library",
  "keywords": [
    "fetch",
    "http",
    "request"
  ],
  "homepage": "https://github.com/gwansikk/query-fetch",
  "bugs": "https://github.com/gwansikk/query-fetch/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gwansikk/query-fetch.git"
  },
  "license": "MIT",
  "author": "Gwansik Kim <gwansik.kim@gwansik.dev>",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest run --coverage",
    "test:watch": "vitest --coverage",
    "check-types": "tsc --noEmit --skipLibCheck"
  },
  "devDependencies": {
    "@query-fetch/utils": "workspace:*"
  },
  "publishConfig": {
    "access": "public"
  }
}
