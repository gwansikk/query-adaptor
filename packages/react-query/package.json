{
  "name": "@query-fetch/react-query",
  "version": "0.1.2",
  "description": "Lightweight and Easy-to-Use Fetch API Extension Library for @tanstack/react-query",
  "keywords": [
    "fetch",
    "http",
    "request",
    "query-fetch",
    "react-query"
  ],
  "homepage": "https://github.com/gwansikk/query-fetch",
  "bugs": "https://github.com/gwansikk/query-fetch/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gwansikk/query-fetch.git"
  },
  "license": "MIT",
  "author": "GwanSik Kim <Seorit98@gmail.com>",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest run --coverage",
    "test:watch": "vitest --coverage",
    "check-types": "tsc --noEmit --skipLibCheck"
  },
  "devDependencies": {},
  "peerDependencies": {
    "@query-fetch/core": "workspace:*",
    "@tanstack/react-query": "^5",
    "react": "^18"
  },
  "publishConfig": {
    "access": "public"
  }
}
