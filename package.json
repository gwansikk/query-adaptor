{
  "name": "@gwansikk/query-fetch",
  "version": "0.6.1",
  "description": "The Query Fetch is a simple and lightweight HTTP Request library.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "watch": "rollup -cw --bundleConfigAsCjs",
    "build": "tsup",
    "test": "vitest",
    "test:run": "vitest run",
    "clean": "rimraf dist",
    "type-check": "tsc --noEmit --skipLibCheck ",
    "docs:dev": "pnpm --filter docs dev",
    "docs:build": "pnpm --filter docs build"
  },
  "homepage": "https://github.com/gwansikk/server-chain",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gwansikk/server-chain.git"
  },
  "author": "gwansikk",
  "license": "MIT",
  "workspaces": [
    "docs"
  ],
  "devDependencies": {
    "@types/eslint": "^8",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "tslib": "^2.6.2",
    "tsup": "^8.1.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.3"
  },
  "keywords": [
    "fetch",
    "http",
    "request"
  ]
}
